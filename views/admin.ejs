<div>
	<h1>We're Loged In</h1>
</div>	

<div class="list-group">
  <!-- loop over blog posts and render them -->
  
  <% reviews.forEach((review) => { %>
    <div class="review-post list-group-item">
      <h4 class="list-group-item-heading"><%= review.firstName %></h4>
      <p class="list-group-item-text">Author: <%= review.lastName %></p>
      <p class="list-group-item-text"><br><%= review.review %></p>
      <p class="list-group-item-text"><br><%= review.score %></p>
      <p class="list-group-item-text"><br><%= review.date %></p>
      <p class="list-group-item-text"><br><%= review.approved %></p>
      <a href="" class="list-group-item-text"><br><%= review._id %></a>
     </div>
  <% }) %>
</div>