
<div class="review-post list-group-item">
  <p class="list-group-item-text">Author:<%= review.firstName %> <%= review.lastName %></p>
  <p class="list-group-item-text"><br><%= review.review %></p>
  <p class="list-group-item-text"><br>
	<% for(var i=0; i<review.score; i++) {%>
	   <i class="fa fa-star" aria-hidden="true"></i>
	<% } %>
  </p>
  <p class="list-group-item-text"><br><%= review.date %></p>
 </div>
